{
  "name": "gitbook-plugin-myseo",
  "version": "1.1.5",
  "description": "This is a GitBook plugin for generate keywords and description for SEO",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies":{
	"html-entities":"2.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WuWenhui/gitbook-plugin-mySEO.git"
  },
  "keywords": [
    "seo",
    "keywords"
  ],
  "author": "wenki",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WuWenhui/gitbook-plugin-mySEO/issues"
  },
  "homepage": "https://github.com/WuWenhui/gitbook-plugin-mySEO#readme",
  
  "engines":{
        "gitbook":">1.x.x"
    },
	"gitbook":{
		"properties":{
			"titlePrefix":{
				"type":"string",
				"default":"",
				"description":"It defines your title prefix"
			},
			"titleSuffix":{
				"type":"string",
				"default":"",
				"description":"It defines your title suffix"
			},
			"description":{
				"type":"string",
				"default":"",
				"description":"It defines your description"
			},
			"keywords":{
				"type":"string",
				"default":"",
				"description":"It defines your keywords"
			}
		}
	}
}
